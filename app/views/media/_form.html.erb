<div>
  <%= form_with model: medium do |f| %>
    <div class="mb-3">
      <label class="form-label"><%= f.label :name %></label>
      <%= f.text_field :name, class: "form-control", placeholder: "Medium Name"%>
    </div>
    <div class="mb-3">
      <label class="form-label"><%= f.label :url %></label>
      <%= f.text_field :url, class: "form-control", placeholder: "Url"%>
    </div>
    <div class="mb-3">
      <label class="form-label"><%= f.label :rss %></label>
      <%= f.text_field :rss, class: "form-control", placeholder: "Rss"%>
    </div>
    <div class="mb-3">
      <%= f.submit class: "btn btn-success" %>
      <%# cancel_path = (medium.persisted? ? edit_medium_path(medium) : media_path) %>
      <%= link_to "Cancel", media_path, class: "btn btn-dark" %>
    </div>
    <% end %>
</div>